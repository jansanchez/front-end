/**
* Icon Module
* @class Icon
* @module css-architecture
*/

.icon
	background url("../images/icon-set.png?version")
	display inline-block
	*display inline
	vertical-align top
	*zoom 1
	&.wheels
		background-position -226px 0
		height 13px
		width 14px
	&.book
		background-position 0 -59px
		height 20px
		width 48px
	&.ilogo
		background-position -48px -59px
		height 12px
		width 60px
	&.help
		background-position -240px 0
		height 16px
		width 16px