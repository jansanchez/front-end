/**
* Font Module
* @class font
* @module css-architecture
*/

@css{

	@font-face{
		font-family:'MyriadWebPro';
		font-weight:normal;font-style:normal
		src:url(../wf/myriadwebpro/myriadwebpro-webfont.eot?#iefix) format('embedded-opentype'),
		src:url(../wf/myriadwebpro/myriadwebpro-webfont.eot?version);
		url(../wf/myriadwebpro/myriadwebpro-webfont.ttf?version) format('truetype');
	}

}