/**
* Container Module
* @class container
* @module css-architecture
*/

#wrapper
	width $WIDTH_PAGE
	padding $PADDING_WRAPPER
	margin 0 auto
	background #FFF

.box-center
	clear both
	margin 0 auto
	position relative
	width widthPage


.content-inner
	padding-bottom 30px
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
.container
	float left
	width 650px
.side-right
	float right
	width 320px